  "apc.activityBar": {
    "position": "bottom",
    "size": 24
  },
  "apc.electron": {
    "frame": false,
    "opacity": 1,
    // "opacity": 1,
    "transparent": true,
    "vibrancy": "ultra-dark",
    "visualEffectState": "active"
  },
  "apc.font.family": "SF Mono",
  "apc.header": {
    "fontSize": 12,
    "height": 24
  },
  "apc.listRow": {
    "fontSize": 13,
    "height": 23
  },
  "apc.menubar.compact": true,
  "apc.monospace.font.family": "SF Mono",
  "apc.sidebar.titlebar": {
    "fontSize": 11,
    "height": 24
  },
  "apc.statusBar": {
    "position": "editor-bottom",
    "size": 24
  },
  "apc.stylesheet": {
    ".explorer-folders-view": {
      "padding-top": "6px"
    },
    ".monaco-findInput .monaco-inputbox": {
      "border-radius": "4px",
      "font-size": "14px",
      "height": "40px",
      "padding-left": "8px"
    },
    ".monaco-list.list_id_4 .monaco-list-rows": {
      "background": "transparent !important"
    },
    ".quick-input-widget": {
      "backdrop-filter": "blur(20px) !important",
      "background": "transparent !important",
      "border": "1px solid rgba(255,255,255,0.1) !important",
      "border-radius": "8px",
      "box-shadow": "0 0 0 100dvw rgba(0,0,0,0.5), 0 10px 100px rgba(0,0,0,0.5) !important",
      "top": "30dvh !important"
    }
  },
  "auto-close-tag.fullMode": false,
  "breadcrumbs.enabled": false,
  "breadcrumbs.filePath": "off",
  "breadcrumbs.showArrays": true,
  "breadcrumbs.showBooleans": true,
  "breadcrumbs.showClasses": true,
  "breadcrumbs.showConstants": true,
  "breadcrumbs.showConstructors": true,
  "breadcrumbs.showEnumMembers": true,
  "breadcrumbs.showEnums": true,
  "breadcrumbs.showEvents": true,
  "breadcrumbs.showFields": true,
  "breadcrumbs.showFiles": true,
  "breadcrumbs.showFunctions": true,
  "breadcrumbs.showInterfaces": true,
  "breadcrumbs.showKeys": true,
  "breadcrumbs.showMethods": true,
  "breadcrumbs.showModules": true,
  "breadcrumbs.showNamespaces": true,
  "breadcrumbs.showNull": true,
  "breadcrumbs.showNumbers": true,
  "breadcrumbs.showObjects": true,
  "breadcrumbs.showOperators": true,
  "breadcrumbs.showPackages": true,
  "breadcrumbs.showProperties": true,
  "breadcrumbs.showStrings": true,
  "breadcrumbs.showStructs": true,
  "breadcrumbs.showTypeParameters": true,
  "breadcrumbs.showVariables": true,
  "chaliceIcons.showArrows": true,
  "commit-message-editor.view.defaultView": "form",
  "commit-message-editor.view.fullWidth": true,
  "commit-message-editor.view.visibleViews": "form",
  "css.lint.unknownAtRules": "ignore",
  "debug.allowBreakpointsEverywhere": true,
  "debug.console.fontFamily": "SF Mono",
  "debug.console.fontSize": 14,
  "debug.showBreakpointsInOverviewRuler": true,
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "always",
  "editor.bracketPairColorization.enabled": false,
  "editor.codeActionsOnSave": {
    "addMissingImports": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "editor.cursorWidth": 4,
  "editor.detectIndentation": true,
  "editor.dragAndDrop": false,
  "editor.fontFamily": "SF Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": "400",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.glyphMargin": true,
  "editor.guides.bracketPairs": false,
  "editor.guides.bracketPairsHorizontal": false,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.guides.highlightActiveIndentation": true,
  "editor.guides.indentation": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.hover.delay": 200,
  "editor.inlineSuggest.enabled": true,
  "editor.insertSpaces": true,
  "editor.largeFileOptimizations": true,
  "editor.lineHeight": 1.25,
  "editor.lineNumbers": "on",
  "editor.linkedEditing": true,
  "editor.matchBrackets": "never",
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": "off",
  "editor.overviewRulerBorder": false,
  "editor.quickSuggestions": {
    "comments": "on",
    "other": "on",
    "strings": "on"
  },
  "editor.renderControlCharacters": false,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "none",
  "editor.scrollbar.horizontalScrollbarSize": 4,
  "editor.scrollbar.verticalScrollbarSize": 4,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": false,
  "editor.suggest.preview": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "explorer.autoReveal": "focusNoScroll",
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.sortOrder": "type",
  "files.autoSave": "off",
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.ignoreRebaseWarning": true,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.views.commitDetails.files.layout": "list",
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "javascript.preferGoToSourceDefinition": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "markdown.preview.fontSize": 14,
  "markdown.preview.lineHeight": 1.5,
  "markdown.preview.scrollEditorWithPreview": false,
  "markdown.preview.scrollPreviewWithEditor": false,
  "markdown.preview.typographer": true,
  "markdownlint.config": {
    "default": true,
    "first-line-heading": false,
    "no-inline-html": false
  },
  "notebook.breadcrumbs.showCodeCells": false,
  "notebook.outline.showCodeCells": true,
  "polacode.shadow": "rgba(0, 0, 0, 0.75) 0px 20px 68px",
  "polacode.transparentBackground": true,
  "prettier.ignorePath": "",
  "prettier.jsxSingleQuote": true,
  "prettier.printWidth": 80,
  "prettier.resolveGlobalModules": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
